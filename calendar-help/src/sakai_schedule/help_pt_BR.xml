<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="scheduleAbout" class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId"><value>arfm_pt_BR</value></property>
		<property name="name"><value>Cronograma</value></property>
		<property name="location"><value>/sakai_schedule/arfm_pt_BR.html</value></property>
		<property name="defaultForTool"><value>sakai.schedule</value></property>
	</bean>
	<bean id="scheduleOverview" class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId"><value>aray_pt_BR</value></property>
		<property name="name"><value>Cronograma: Resumo</value></property>
		<property name="location"><value>/sakai_schedule/aray_pt_BR.html</value></property>
	</bean>
	<bean id="scheduleSummary"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>avaz_pt_BR</value>
		</property>
		<property name="name">
			<value>Resumo do Cronograma</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/avaz_pt_BR.html</value>
		</property>
		<property name="defaultForTool">
			<value>sakai.summary.calendar</value>
		</property>
	</bean>
	<bean id="addActivity"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqyn_pt_BR</value>
		</property>
		<property name="name">
			<value>Adicionar, editar ou deletar item</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqyn_pt_BR.html</value>
		</property>
	</bean>
	<bean id="addActivityAttachment"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>argn_pt_BR</value>
		</property>
		<property name="name">
			<value>Adicionar anexo ao item</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/argn_pt_BR.html</value>
		</property>
	</bean>
	<bean id="viewSchedule"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqzh_pt_BR</value>
		</property>
		<property name="name">
			<value>Visualizar detalhes do item</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqzh_pt_BR.html</value>
		</property>
	</bean>

	<bean id="changeScheduleView"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqyp_pt_BR</value>
		</property>
		<property name="name">
			<value>Alterar visualização</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqyp_pt_BR.html</value>
		</property>
	</bean>

	<bean id="addCalendarFields"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqzc_pt_BR</value>
		</property>
		<property name="name">
			<value>Adicionar ou deletar campos do calendário</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqzc_pt_BR.html</value>
		</property>
	</bean>

	<bean id="printCalendar"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>arfe_pt_BR</value>
		</property>
		<property name="name">
			<value>Imprimir calendário</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/arfe_pt_BR.html</value>
		</property>
	</bean>

	<bean id="mergeCalendar"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqyl_pt_BR</value>
		</property>
		<property name="name">
			<value>Compartilhar calendários</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqyl_pt_BR.html</value>
		</property>
	</bean>
	
	<bean id="importCalendar"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>argo_pt_BR</value>
		</property>
		<property name="name">
			<value>Importar um calendário</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/argo_pt_BR.html</value>
		</property>
	</bean>


	<bean id="org.sakaiproject.api.app.help.TableOfContents"
		class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
		<property name="name">
			<value>root</value>
		</property>
		<property name="categories">
			<list>
				<bean id="scheduleCategory"
					class="org.sakaiproject.component.app.help.model.CategoryBean">
					<property name="name">
						<value>Cronograma</value>
					</property>
					<property name="resources">
						<list>
							<ref bean="scheduleAbout" />
							<ref bean="scheduleOverview" />
							<ref bean="scheduleSummary" />
							<ref bean="addActivity" />
							<ref bean="addActivityAttachment" />
							<ref bean="viewSchedule" />
							<ref bean="addCalendarFields" />
							<ref bean="changeScheduleView" />
							<ref bean="printCalendar" />
							<ref bean="mergeCalendar" />
							<ref bean="importCalendar" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>
</beans>
